##CLEARCASE CMDS
function msv {
    csv ${1%.*}
}

function mrv {
    rmv ${1%.*}
}

function ls_tag {
	ct pwv | awk '{print $NF}' | tail -1
}

function mco {
	ct co -nc $1
}

alias cdv="cd /home/oakeada/views; l"
alias cdvobs="cd /view/$(ls_tag)/vobs"
alias edcs="ct edcs"
alias catcs="ct catcs"
alias setcs="ct setcs"
